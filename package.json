{
  "name": "afkalc",
  "version": "1.4.6",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "yum install libuuid-devel libmount-devel zlib && cp -v /lib64/{libuuid,libmount,libblkid,libz}.so.1 node_modules/canvas/build/Release/ && npm run build",
    "start": "next start",
    "extract": "i18next --config i18next-parser.config.js",
    "lint": "npm run lint:css && npm run lint:js && npm run lint:ts",
    "lint:js": "eslint --fix --ext .ts,.tsx ./",
    "lint:css": "stylelint --fix \"**/*.css\"",
    "lint:ts": "tsc -p tsconfig.json",
    "prettier": "prettier --write '**/*.@(ts|tsx|js|jsx|css)'",
    "test": "jest --watch",
    "test:lint-staged": "cross-env CI=true jest --findRelatedTests",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@mdi/js": "^6.5.95",
    "@testing-library/react-hooks": "^7.0.2",
    "canvas": "^2.6.1",
    "dayjs": "^1.10.7",
    "firebase": "^8.4.3",
    "i18next": "^21.6.12",
    "nanoid": "^3.3.1",
    "next": "12.1.0",
    "next-i18next": "^10.4.0",
    "qrcode.react": "^1.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.15.5",
    "sharp": "^0.30.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/qrcode.react": "^1.0.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run test:lint-staged",
      "eslint --ext .js,.jsx,.ts,.tsx",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
